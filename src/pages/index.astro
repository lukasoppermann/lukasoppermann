---
import Layout from '../layouts/Layout.astro';
import Intro from '../components/home/Intro.astro';
import Resume from '../components/home/Resume.astro';
import HomepageLink from '../components/home/HomepageLink.astro';
import Project from '../components/home/Project.astro';
import { getCollection } from "astro:content";
const homepageLinks = await getCollection("homepageLinks");
const projects = await getCollection("projects");

---
<style>

</style>

<Layout>
  <Intro />
  <Resume />
  {projects.map(async (project) => {
    const { Content}  = await project.render()
    return <Project {...project.data} Content={Content}} />})}
  {homepageLinks.map(link => <HomepageLink {...link.data} />)}
</Layout>


